<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>SNS Servlet Example</display-name>
	<servlet>
		<servlet-name>SNSServlet</servlet-name>
		<display-name>SNSServlet</display-name>
		<description>Test SNS Servlet</description>
		<servlet-class>com.test.servlet.SNSServlet</servlet-class>
		 <load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>SNSServlet</servlet-name>
		<url-pattern>/SNSServlet</url-pattern>
	</servlet-mapping>
  
</web-app>
